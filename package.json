{
  "name": "restaurant_api",
  "version": "1.0.0",
  "description": "This is an API for restaurant",
  "main": "dist",
  "scripts": {
    "dev": "NODE_ENV=development nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --preset es2015, stage-0",
    "start": "NODE_ENV=production pm2 start dist",
    "prestart":"npm run -s build",
    "lint":"eslint src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "eslintConfig":{
    "parserOptions":{
      "ecmaVersion": 7,
      "sourceTop": "module"
    },
    "env":{
      "node":true
    },
    "rules":{
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "author": "i3lackAngel",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.3",
    "eslint": "^5.15.3",
    "express": "^4.16.4",
    "mongoose": "^5.4.19",
    "nodemon": "^1.18.10"
  }
}
